            <div class="groupinfo radioBox hide"><div class="cont" style="color:white;">善行创投邀请你加入了“亿万富豪红包群”群聊，群聊参与人还有：马化腾 腾讯、善行创投、柳传志 联想、刘强东 京东……</div></div>

            <div id="msg1" class="massge hide" style="height: 153px;">
                <div class="headimg"><img src="/images/ntx1.jpg" /></div>
                <div class="mname">马化腾 腾讯</div>
                <div class="mcontent">
                    <div class="mcla radioBox">刚才群里有人发了个大红包啊，我也来凑个热闹。</div>
                    <div class="jb"><img src="/images/pdj.png" /></div>
                </div>
            </div>

            <div id="msg2" class="massge hide" style="height: 120px;">
                <div class="headimg"><img src="/images/ntx1.jpg" /></div>
                <div class="mname">马化腾 腾讯</div>
                <div class="mcontent">
                    <div class="mcla radioBox">我看看我微信支付还有多少钱哈~</div>
                    <div class="jb"><img src="/images/pdj.png" /></div>
                </div>
            </div>

            <div id="msg3" class="massge hide" style="height: 233px;">
                <div class="headimg"><img src="/images/ntx1.jpg" /></div>
                <div class="mname">马化腾 腾讯</div>
                <div class="redpick" id="redpick1">
                    <img src="/images/redpick.png" />
                </div>
                <img class="circle"  src="/images/circle.png" />
                <img class="hand"  src="/images/hand.png" />
            </div>
        <div class="msg_next">
            <div id="msg4" class="massge hide" style="height: 184px;">
                <div class="headimg"><img src="/images/tx3.jpg" /></div>
                <div class="mname">善行创投</div>
                <div class="mcontent">
                    <div><img src="/images/nbq5.jpg" /></div>
                </div>
            </div>

            <div id="msg5" class="massge hide" style="height: 120px;">
                <div class="headimg"><img src="/images/tx3.jpg" /></div>
                <div class="mname">善行创投</div>
                <div class="mcontent">
                    <div class="mcla radioBox"  style="width: 243px!important;">嘿嘿，又是我抢了<img src="/images/bq1.png" style="position: absolute;margin: 0 0 0 11px;width: 34px;"/><img src="/images/bq1.png" style="position: absolute;margin: 0 0 0 11px;width: 34px;"/><img src="/images/bq1.png" style="position: absolute;margin: 0 0 0 11px;width: 34px;"/></div>
                    <div class="jb"><img src="/images/pdj.png" /></div>
                </div>
            </div>

            <div id="msg6" class="massge hide" style="height: 160px;">
                <div class="headimg"><img src="/images/tx7.jpg" /></div>
                <div class="mname">柳传志 联想</div>
                <div class="mcontent">
                    <div class="mcla radioBox">小善啊，你就不能好好放标么！<img src="/images/nbq2.png" style="margin: 0 0 0 10px;width: 34px;"/>看见钱就抢，手挺快啊。</div>
                    <div class="jb"><img src="/images/pdj.png" /></div>
                </div>
            </div>

            <div id="msg7" class="massge hide" style="height: 136px;">
                <div class="headimg"><img src="/images/tx3.jpg" /></div>
                <div class="mname">善行创投</div>
                <div class="mcontent">
                    <div class="mcla radioBox" id="sanlianfa"><img src="/images/nbq3.png"/><img src="/images/nbq3.png"/><img src="/images/nbq3.png"/></div>
                    <div class="jb"><img src="/images/pdj.png" /></div>
                </div>
            </div>

            <div id="msg8" class="massge hide" style="height: 168px;">
                <div class="headimg"><img src="/images/tx3.jpg" /></div>
                <div class="mname">善行创投</div>
                <div class="mcontent">
                    <div class="mcla radioBox">错了，错了，这就把抢到的红包发出来，好好开会<img class="msgimg" src="/images/nbq4.png"/></div>
                    <div class="jb"><img src="/images/pdj.png" /></div>
                </div>
            </div>

            <div id="msg9" class="massge hide" style="height: 220px;">
                <div class="headimg"><img src="/images/tx3.jpg" /></div>
                <div class="mname">善行创投</div>
                <div class="redpick" id="redpick2">
                    <img src="/images/redpick.png" />
                </div>
                <img class="circle"  src="/images/circle.png" />
                <img class="hand"  src="/images/hand.png" />
            </div>
         <div class="clear"></div>
            <div class="di"><img src="/images/di.jpg"/></div>

        </div>
     </div>
    </div>

    <div id="hongbao" class="hongbao hide">
        <img id="hb1" class="hide" src="/images/hb1.jpg" />
        <img id="hb1Open" class="hide" src="/images/hb1_open.jpg" />
        <div class="btn1"></div>
        <div id="btn2"></div>
        <div id="btn3" ></div>
        <img class="circle hide"  src="/images/circle.png" />
        <img class="hand hide"  src="/images/hand.png" />
    </div>
    <div id="red_bao" class="hongbao hide">
        <img id="hb2" class="hide" src="/images/hb2.jpg" />
        <div class="btn_hb"></div>
        <div class="envelope_red"></div>
    <!--
        <div ><a href="sx.html"  class="envelope_red"></a></div>
    -->
    </div>
    <div id="hbnull" class="hbnull hide">
        <img id="null1" class="hide" src="/images/hb1_null.jpg" />
        <img class="circle hide" src="/images/circle.png" />
        <img class="hand hide"  src="/images/hand.png" />
    </div>
    <div  class="hbnull hide">
    <img id="sx_info" class="hide" src="/images/sx.jpg" />
    </div>

</div>
<div class="clear"></div>
<script>
 $(function(){
         $(".radioBox").delay("slow").fadeIn();
         $("#msg1").delay("2000").fadeIn();
         $("#msg2").delay("3000").fadeIn();
         $("#msg3").delay("4000").fadeIn();

     $("#redpick1 img").click(function(){
         $("#hongbao").removeClass("hide");
         $("#hb1").removeClass("hide");
     });
     $("#redpick2").click(function(){
         $("#redpick1").addClass("hide");
         $("#red_bao").removeClass("hide");
         $("#hb2").removeClass("hide");
     });

     $("#btn2").click(function(){
         $("#hongbao").removeClass("hide");
         $("#hb1Open").removeClass("hide");
         setTimeout("$('#hongbao .hand').removeClass('hide')",2000);
         setTimeout("$('#hongbao .circle').removeClass('hide')",2000);
     });
    $(".envelope_red").click(function(){
       $(".hbnull").removeClass("hide");
        $("#hb2").addClass("hide");
        $("#sx_info").removeClass("hide");

    });
    $("#sx_info").click(function(){
        $(".hbnull").addClass("hide");
        $("#red_bao").addClass("hide");

        $(this).addClass("hide");
    });

     $("#btn3").click(function(){
         $("#hbnull").removeClass("hide");
         $("#null1").removeClass("hide");
         $("#hb1Open").addClass("hide");
         $("#hb1").addClass("hide");
         $("#hongbao .hand").addClass("hide");
         $("#hongbao .circle").addClass("hide");
         setTimeout("$('#hbnull .hand').removeClass('hide')",5000);
         setTimeout("$('#hbnull .circle').removeClass('hide')",5000);
      /*   $("#hbnull .hand").removeClass("hide");
         $("#hbnull .circle").removeClass("hide");*/

     });
     $(".btn1").click(function(){

         $("#hb1").addClass("hide");
         $("#hb1Open").addClass("hide");
         $("#hongbao").addClass("hide");
         $(".hand").addClass("hide");
         $(".circle").addClass("hide");
         $("#msg4").delay("slow").fadeIn();
         $(".page").css({"position":"fixed","top":"-450px"});
         $("#msg5").delay("2000").fadeIn();
         $("#msg6").delay("3000").fadeIn();
         $("#msg3").delay("3000").fadeOut(1000);
         $("#msg7").delay("4000").fadeIn();
         $("#msg4").delay("4000").fadeOut(1000);
         $("#msg8").delay("5000").fadeIn();
         $("#msg9").delay("6000").fadeIn();

     });
     $(".btn_hb").click(function(){
         $("#hb2").addClass("hide");
         $(".hongbao").addClass("hide");
     });
     $("#null1").click(function(){
         $("#hongbao").addClass("hide");
         $("#hbnull").addClass("hide");
         $("#null1").addClass("hide");
         $(".hand").addClass("hide");
         $(".circle").addClass("hide");
         $("#msg4").delay("slow").fadeIn();
         $(".page").css({"position":"fixed","top":"-450px"});
         $("#msg5").delay("2000").fadeIn();
         $("#msg6").delay("3000").fadeIn();
         $("#msg3").delay("3000").fadeOut(1000);
         $("#msg7").delay("4000").fadeIn();
        $("#msg4").delay("4000").fadeOut(1000);
         $("#msg8").delay("5000").fadeIn();
         $("#msg9").delay("6000").fadeIn();

     });

 })
 var baseUrl = '<%configureProvider.format(out,URLVariable.INDEX);%>';
 var shareData = {
	  title: '善行创投邀请你加入群聊',
	  desc: '善行创投邀请你加入群聊亿万富豪红包群，进入查看详情。',
	  link: baseUrl + '/mobile/hb.html',
	  imgUrl: baseUrl + '/images/hbshare.jpg'
	};
 function registerShareAppMessage(){
	 wx.hideMenuItems({
			menuList : [ 'menuItem:readMode', // 阅读模式
			'menuItem:share:weiboApp', // 分享到腾讯微博
			'menuItem:openWithQQBrowser', // 使用qq浏览器打开
			'menuItem:openWithSafari', // 使用safari浏览器打开
			'menuItem:setFont', // 设置字体
			'menuItem:originPage', // 原网页
			'menuItem:favorite', // 收藏
			'menuItem:share:email', // 发送邮件
			'menuItem:exposeArticle', // 举报
			'menuItem:copyUrl' // 复制链接
			]
	 });
	 wx.onMenuShareAppMessage(shareData);
	 wx.onMenuShareTimeline(shareData);
	 wx.onMenuShareQQ(shareData);
	 wx.onMenuShareQZone(shareData);
 }
</script>
<%@include file="/WEB-INF/include/weixinShare.jsp" %>
</body>
</html>